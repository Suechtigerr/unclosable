set x=createobject("wscript.shell")

x.sendkeys "^"+"{ESC}"
wscript.sleep 1000
x.sendkeys "command prompt"
wscript.sleep 1000
x.sendkeys "{ENTER}"
wscript.sleep 500
x.sendkeys "start https://www.youtube.com/watch?v=dQw4w9WgXcQ"
wscript.sleep 500
x.sendkeys "{ENTER}"
wscript.sleep 4000
x.sendkeys "{BS}" 
